{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "sborka-common",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "sborka-common",
      "kebab-case"
    ],
    "quotemark": false,
    "prefer-const": false,
    "max-line-length": [
      true,
      145
    ]
  }
}
